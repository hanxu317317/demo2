<% if (process.env.NODE_ENV === 'production') {  %>

<!-- jQuery、json3 -->
<script src="/assets/jquery.min.js"></script>
<script src="/assets/jquery.mobile.min.js"></script>


<!-- 每个页面对应的自己的 js -->
<% if(typeof filename === 'string'){ %>
<script src="/assets/<%= filename %>.min.js?_=<%= global.fileHash %>"></script>
<script src="/assets/global.min.js?_=<%= global.fileHash %>"></script>
<script src="/assets/jiaohu.min.js?_=<%= global.fileHash %>"></script>
<% } %>
<% } else { %>
<!-- jQuery -->
<script src="/public/lib/jquery-2.1.4.js"></script>
<script src="/public/lib/jquery.mobile.js"></script>
<!-- 每个页面对应的自己的 js -->
<% if(typeof filename === 'string'){ %>
<script src="/public/js/<%= filename %>.js"></script>
<script src="/public/js/global.js"></script>
<script src="/public/js/jiaohu.js"></script>
<% } %>

<% } %>


